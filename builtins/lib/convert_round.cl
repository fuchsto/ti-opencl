_CLC_DEF _CLC_OVERLOAD
float convert_float_rtz(uint x)
{
#ifdef USE_C66_HARDWARE_ROUNDING
  c66_set_round(FE_TOWARDZERO);
  float r66 = convert_float(x); // inlining will cause wrong scheduling
  c66_set_round(FE_TONEAREST);
  return r66;
#else
  float r = convert_float(x);
  uint y = convert_uint(r);
  uint abs_x = abs(x);
  uint abs_y = abs(y);
  return select(r, nextafter(r, sign(r) * (float)-INFINITY), convert_int(abs_y > abs_x));
#endif
}

