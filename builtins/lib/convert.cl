float convert_float(double x)
{
  if (((as_ulong(x) >> 52) & 0x7FF) <= 0x380) // biased - 0x3FF <= -0x7F
  {
    if ((as_ulong(x) & 0x8000000000000000) == 0) return  0.0f;
    else                                         return -0.0f;
  }
  return (float)x;
}

